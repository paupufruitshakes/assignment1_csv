def a():
    
    import csv

    energy_list = []

    with open('Assignment1_data.csv', 'r') as f:
        reader = csv.reader(f, delimiter = ',', quoting = csv.QUOTE_ALL)
        for line in reader:
            energy_list.append(line[0])

    print(energy_list) 
      
      
def b():
    
    import csv

    state_list = []

    with open('Assignment1_data.csv', 'r') as f:
        reader = csv.reader(f, delimiter = ',', quoting = csv.QUOTE_ALL)
        for line in reader:
            state_list.append(line[1])

    print(state_list)
   
   
def c():
    
    import csv

    BTU_list = []

    with open('Assignment1_data.csv', 'r') as f:
        reader = csv.reader(f, delimiter = ',', quoting = csv.QUOTE_ALL)
        for line in reader:
            BTU_list.append(line[2])

    print(BTU_list) 
